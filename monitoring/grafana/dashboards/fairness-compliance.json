{
  "dashboard": {
    "id": null,
    "title": "Fairness & Compliance Monitoring",
    "tags": ["fairness", "bias", "compliance", "ethics"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Demographic Parity",
        "type": "stat",
        "targets": [
          {
            "expr": "fairness:demographic_parity_1h",
            "legendFormat": "Demographic Parity"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 3,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Equal Opportunity",
        "type": "stat",
        "targets": [
          {
            "expr": "fairness:equal_opportunity_1h",
            "legendFormat": "Equal Opportunity"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 3,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Disparate Impact Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "fairness:disparate_impact_1h",
            "legendFormat": "Disparate Impact"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 3,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "red", "value": 0.7},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.9},
                {"color": "green", "value": 1.1},
                {"color": "yellow", "value": 1.2},
                {"color": "red", "value": 1.3}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Overall Fairness Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "(fairness:demographic_parity_1h + fairness:equal_opportunity_1h + fairness:equalized_odds_1h) / 3",
            "legendFormat": "Fairness Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.8}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Bias Metrics by Protected Attribute",
        "type": "graph",
        "targets": [
          {
            "expr": "fairness:bias_score_by_attribute{protected_attribute=\"gender\"}",
            "legendFormat": "Gender Bias"
          },
          {
            "expr": "fairness:bias_score_by_attribute{protected_attribute=\"race\"}",
            "legendFormat": "Race Bias"
          },
          {
            "expr": "fairness:bias_score_by_attribute{protected_attribute=\"age\"}",
            "legendFormat": "Age Bias"
          }
        ],
        "yAxes": [
          {
            "label": "Bias Score",
            "min": 0,
            "max": 1
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.1],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "last"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "Bias Violation Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Fairness Metrics Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "fairness:demographic_parity_1h",
            "legendFormat": "Demographic Parity"
          },
          {
            "expr": "fairness:equal_opportunity_1h",
            "legendFormat": "Equal Opportunity"
          },
          {
            "expr": "fairness:equalized_odds_1h",
            "legendFormat": "Equalized Odds"
          }
        ],
        "yAxes": [
          {
            "label": "Fairness Score",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Compliance Status",
        "type": "table",
        "targets": [
          {
            "expr": "compliance_status{regulation=\"FCRA\"}",
            "legendFormat": "FCRA",
            "format": "table"
          },
          {
            "expr": "compliance_status{regulation=\"ECOA\"}",
            "legendFormat": "ECOA",
            "format": "table"
          },
          {
            "expr": "compliance_status{regulation=\"GDPR\"}",
            "legendFormat": "GDPR",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "indexByName": {},
              "renameByName": {
                "regulation": "Regulation",
                "Value": "Status"
              }
            }
          }
        ],
        "gridPos": {"h": 9, "w": 8, "x": 0, "y": 17}
      },
      {
        "id": 8,
        "title": "Model Decisions by Demographics",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (decision, demographic) (model_decisions_total)",
            "legendFormat": "{{decision}} - {{demographic}}"
          }
        ],
        "gridPos": {"h": 9, "w": 8, "x": 8, "y": 17}
      },
      {
        "id": 9,
        "title": "Privacy Budget Consumption",
        "type": "graph",
        "targets": [
          {
            "expr": "privacy_budget_consumed",
            "legendFormat": "Privacy Budget Used"
          },
          {
            "expr": "privacy_budget_total",
            "legendFormat": "Total Privacy Budget"
          }
        ],
        "yAxes": [
          {
            "label": "Privacy Budget",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 8, "x": 16, "y": 17}
      },
      {
        "id": 10,
        "title": "Audit Trail Events",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"credit-risk-api\"} |= \"audit\"",
            "legendFormat": "Audit Events"
          }
        ],
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 26}
      }
    ]
  }
}