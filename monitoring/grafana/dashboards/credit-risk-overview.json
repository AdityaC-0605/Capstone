{
  "dashboard": {
    "id": null,
    "title": "Credit Risk AI - System Overview",
    "tags": ["credit-risk", "ai", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(api:request_rate_5m)",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "API Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(api:error_rate_5m) * 100",
            "legendFormat": "Error %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "API Latency (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "api:latency_p95_5m * 1000",
            "legendFormat": "P95 Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 200}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Model Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "model:accuracy_1h * 100",
            "legendFormat": "Accuracy %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 85}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Request Volume Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(api:request_rate_5m)",
            "legendFormat": "Total Requests/sec"
          },
          {
            "expr": "sum(api:prediction_rate_5m)",
            "legendFormat": "Predictions/sec"
          },
          {
            "expr": "sum(api:batch_prediction_rate_5m)",
            "legendFormat": "Batch Predictions/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per second",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"credit-risk-api\"}[5m])) * 1000",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"credit-risk-api\"}[5m])) * 1000",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"credit-risk-api\"}[5m])) * 1000",
            "legendFormat": "P99"
          }
        ],
        "yAxes": [
          {
            "label": "Response time (ms)",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Model Performance Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "model:accuracy_1h * 100",
            "legendFormat": "Accuracy %"
          },
          {
            "expr": "model:precision_1h * 100",
            "legendFormat": "Precision %"
          },
          {
            "expr": "model:recall_1h * 100",
            "legendFormat": "Recall %"
          },
          {
            "expr": "model:f1_score_1h * 100",
            "legendFormat": "F1 Score %"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 17}
      },
      {
        "id": 8,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(infrastructure:cpu_utilization_5m) * 100",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "avg(infrastructure:memory_utilization_5m) * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 17}
      }
    ]
  }
}